:@template"\@// Your custom settings file to override Foundation defaults
@import "settings";

// Comment out this import if you are customizing you imports below
//@import "foundation";

// ----------------------------------------
// Import specific parts of Foundation by commenting the import "foundation"
// and uncommenting what you want below. You must uncomment the following if customizing

@import "compass/css3";
@import "foundation/settings";
@import "foundation/functions/all";

// Control which mixins you have access too

@import "foundation/mixins/clearfix";
@import "foundation/mixins/css-triangle";
@import "foundation/mixins/font-size";

// Must include next two for semantic grid to work

@import "foundation/mixins/respond-to";
@import "foundation/mixins/semantic-grid";

@import "modular-scale";
@import "foundation/common/globals";

// Must include the grid for any responsiveness

@import "foundation/components/grid";

// Control which common styles get compiled

@import "foundation/common/typography";
@import "foundation/common/forms";

// Control which components you get if customizing

@import "foundation/components/modules/buttons";
@import "foundation/components/modules/tabs";
@import "foundation/components/modules/ui";
@import "foundation/components/modules/topbar";
@import "foundation/components/modules/navbar";
@import "foundation/components/modules/orbit";
@import "foundation/components/modules/reveal";
@import "foundation/components/modules/offcanvas";
@import "foundation/components/modules/clearing";
@import "foundation/components/modules/joyride";

// Media Queries Overrides
// @import "foundation/components/modules/mqueries";

@import "compass/css3/border-radius";
@import "compass/css3/opacity";
@import "compass/css3/text-shadow";


html,
body {
	margin: 0;
	padding: 0;	
	background-color: #fff;
}


/* Generated by Font Squirrel (http://www.fontsquirrel.com) on February 9, 2013 */



@font-face {
    font-family: 'nokia_pure_textregular';
    src: url('../fonts/nokiapuretext-webfont.eot');
    src: url('../fonts/nokiapuretext-webfont.eot?#iefix') format('embedded-opentype'),
         url('../fonts/nokiapuretext-webfont.woff') format('woff'),
         url('../fonts/nokiapuretext-webfont.ttf') format('truetype'),
         url('../fonts/nokiapuretext-webfont.svg#nokia_pure_textregular') format('svg');
    font-weight: normal;
    font-style: normal;

}




@font-face {
    font-family: 'nokia_pure_headlinebold';
    src: url('../fonts/nokiapureheadlinebd-webfont.eot');
    src: url('../fonts/nokiapureheadlinebd-webfont.eot?#iefix') format('embedded-opentype'),
         url('../fonts/nokiapureheadlinebd-webfont.woff') format('woff'),
         url('../fonts/nokiapureheadlinebd-webfont.ttf') format('truetype'),
         url('../fonts/nokiapureheadlinebd-webfont.svg#nokia_pure_headlinebold') format('svg');
    font-weight: normal;
    font-style: normal;

}

// primary wrappers 
// to organise masks and z-indexs

.main-wrapper {	color: #fff; display: none;}
.mid-wrapper {	width: 100%;height: 1px;overflow: hidden;}

#loading-page.main-wrapper {display: block !important;color: #ffffff; text-align: center; margin-top: 120px;}

#master-wrapper { z-index: 1000000000000; position: absolute; top: 0; left: 0; width: 100%; height: 1100px; overflow: hidden;}

#background-image-panel { z-index: 1; }
#background-image-panel .background-image-main { position: absolute; top: 0; left: 0; width: 100%; height: 720px; background-color: #d10804;}
#background-image-panel .background-image-video { background-image: url('../images/bg_vid_strip_firass.png'); background-size: 100% 100%; text-align: center; @include opacity(0); position: absolute; top: 0; left: 0; width: 100%; height: 1060px; background-color: #020203; z-index: 99999999999999999999;}
#background-image-panel .background-image-video img{ height: 100%; width: auto; margin: 0 auto; }

.relative-wrapper {position: relative; }


// menu at top of page
nav {font-family: $headerFontFamily; border-width:1px 0;    list-style:none;    margin:0;    padding:0;    text-align:center; height:40px;}
nav li {display:inline; @include font-size(12);}
nav .faded {color: #fff;  @include opacity(0.3);}
nav a {display:inline-block;    color: #fff;  @include opacity(0.6); margin-left: 15px; margin-right: 15px;}
nav ul li a:link {}
nav ul li a:hover {padding-bottom:3px;	  border-bottom:solid 1px #ffffff;	color: #fff;    @include opacity(1); }
nav ul li a.active {  color: #fff;  @include opacity(1); }

header {display: block;	height:90px; }

// logo
.header-logo-wrapper {  text-align: center; display: table; width: 100%; height: 45px; padding-bottom: 15px;}
.header-logo-wrapper > span, .header-logo-wrapper:before, .header-logo-wrapper:after {    display: table-cell;}
.header-logo-wrapper:before, .header-logo-wrapper:after { background: url(../images/line_white.png) repeat-x center; width: 50%; content: ' '; }
.header-logo-wrapper > span { white-space: nowrap; padding: 0 9px;}





// homepage
#home-page .top-row { height: 270px; width: 100%; overflow: hidden;}
#home-page .bottom-row { height: 225px;  /*border-top: solid 2px #ffffff;*/ width: 100%; overflow: none; }
#home-page h1 { height: 80px; @include font-size(36); padding-top: 25px; text-align: center; overflow: hidden; }
#home-page p { @include font-size(12); text-align: center;}


#home-page .top-row .home-person-1, 
#home-page .top-row .home-person-2, 
#home-page .top-row .home-person-3, 
#home-page .top-row .home-person-4 { display: none; } 

#home-page .top-row .intro-row,
#home-page .top-row .video-row { position: relative; height: 65px; }
#home-page .top-row .base { position: absolute; bottom: 0; left: 0; }
#home-page .top-row .intro-row .base { font-family: $headerFontFamily; @include font-size(22); text-align: center; width: 100%; }
#home-page .top-row .video-row .base { font-family: $headerFontFamily; @include font-size(14);  margin-bottom: 5px; }
#home-page .top-row .answer-row-1 { width: 100%; height: 50px; text-align: center; }
#home-page .top-row .answer-row-2 { width: 100%; text-align: center; }

#home-page .top-row .video-wrapper { position: relative; }

#home-page .top-row .phone-style {position: absolute; top:65px; left: 0px; width: 200px; height: 105px;  }
#home-page .top-row .phone-thumbs {position: absolute; top:75px; left: 24px; width: 152px; height: 84px; background-color: #000; z-index: 5000000; }
#home-page .top-row .phone-play-button-img {position: absolute; top:77px; left: 60px; width: 80px; height: 80px; z-index: 6000000; }
#home-page .top-row .phone-play-button-img img {width: 100%; height: 100%; }
#home-page .top-row .phone-play-button {position: absolute; top:45px; left: 0px; width: 200px; height: 130px; z-index: 7000000; cursor: pointer; }
#home-page .top-row .phone-shadow {position: absolute; top:45px; left: 0px; width: 260px; height: 150px; background-image: url('../images/phone_shadow.png')}
#home-page .top-row .more-videos { font-family: $headerFontFamily; @include font-size(11); text-transform:uppercase; position: absolute; top:177px; left: 0px; color: #fff;}
#home-page .top-row .more-videos a:link { color: #fff; text-decoration: none;}
#home-page .top-row .more-videos a:visited { color: #fff; text-decoration: none;}
#home-page .top-row .more-videos a:hover { color: #fff; text-decoration: underline;}

#home-page .divider-line { z-index: 2; background-color:#fff; width:100%; height:2px;}

#home-page .bottom-row .pointer { font-family: $headerFontFamily; float: left; width: 15px;}
#home-page .bottom-row .texter { font-family: $headerFontFamily; float: left; cursor: pointer;}
#home-page .bottom-row .enter { position: absolute; top: 0px; left: 0px; height: 24px; width: 200px; @include font-size(16); overflow: hidden; @include single-text-shadow(rgba(0,0,0,.2), 4px, 4px, 0);}
#home-page .bottom-row .location { position: absolute; top: 20px; left: 0px; height: 20px; width: 200px; @include font-size(12); overflow: hidden; @include single-text-shadow(rgba(0,0,0,.2), 4px, 4px, 0);}


// features page
#features-page .features-details { margin-top: 40px; width: 100%; height: 375px; overflow: hidden;}
#features-page .features-background { position: absolute; top: 0; left: 0; width: 100%; height: 375px; background-color: #efefef; overflow: hidden;}

#features-page .panel-wrapper { position: absolute; top: 0; left: 0; width: 100%; height: 375px; color: #000;}
#features-page .image-target { position: absolute; top: 0; left: 0; width: 50%; height: 375px; }

#features-page .content-wrapper {top: 0; left: 50%; width: 40%; height: 100%;  position: absolute; display: block;}
#features-page .vertical-wrapper {top: 0; left: 0; width: 100%; height: 100%;  position: absolute; display: table;}
#features-page .line-wrapper {border-left: 1px solid #e1e1e1; padding-left: 40px;}

#features-page .content-target { display: block; margin-top: 70px;  }
#features-page .content-target .kicker { margin-bottom: 15px; color:#515050; font-size: 11px; }
#features-page .content-target .title { font-family: $headerFontFamily; margin-bottom: 15px; color:#515050; font-size: 24px; }
#features-page .content-target .body { margin-bottom: 0px; color:#515050; font-size: 14px; line-height: 22px; }



// video page
#video-panel {	display: none;	background-color: #ffffff;	padding: 0px;	margin-bottom: 10px;	margin-top: 30px;	width: 100%; }

#video-page .video-caption { font-family: $headerFontFamily; @include font-size(12); color: #fff; text-align: center; text-transform: uppercase; width: 100%; padding: 12px; }

#video-page .submenu-wrapper { width: 100%;  height: 150px; background-color: #000; padding-top: 20px; margin-top: 30px; border-top: 1px solid rgba(255, 255, 255, 0.2)}
#video-page a {@include font-size(11); color: #fff; @include opacity(0.3); }
#video-page a img { width: 100%; height: auto; border: 1px solid #fff; margin-bottom: 10px;}
#video-page a:hover {@include opacity(1); }
#video-page a.active {@include opacity(1); }


// footer
#footer-panel {	display: none;	margin: 30px auto; }

#footer-panel .panel-wrapper {height: 225px; width: 100%; }

#footer-panel .footer-container { display:block; width:100%; height: 225px; background-color: #fff; }

#footer-panel .footer-container-mask { display:block; width:950px; height: 225px; background-color: #fff; overflow: hidden; }
#footer-panel .footer-container-slide { display:block; width:1900px; height: 225px; background-color: #fff;  }

#footer-panel .footer-title {font-family: $headerFontFamily; color: #3e3e3e; @include single-text-shadow(rgba(0,0,0,.2), 2px, 2px, 0); @include font-size(14); height: 40px; width: 100%; border-bottom: #e8e8e8 solid 2px; margin-bottom: 15px;text-align: center; margin-top: 25px; }
#footer-panel .footer-caption { color: #474747; @include font-size(12); line-height: 15px; }
#footer-panel .footer-thumb, #footer-panel .footer-intro { color: #474747; @include font-size(12); line-height: 15px; height: 85px; }
#footer-panel .footer-thumb-target { height: 78px; border: 1px solid #6c6c6c; background-color: #efefef; }

#footer-panel .footer-container a { color: #3c3c3c; }
#footer-panel .footer-container a:hover { text-decoration: underline; }
#footer-panel .footer-container a.active .footer-caption { @include opacity(0.5); }

#footer-panel .footer-arrow-left { background-image: url(../images/features_arrow_left.png); display: block; width: 22px; height: 22px; position: absolute; left: 50%; margin-left: -508px; top: 80px; cursor: pointer; }
#footer-panel .footer-arrow-right { background-image: url(../images/features_arrow_right.png); display: block; width: 22px; height: 22px; position: absolute; left: 50%; margin-left: 486px; top: 80px; cursor: pointer;  }



// side nav buttons
#image-nav-panel { position: absolute; overflow:hidden; top: 157px; left: 0; width: 100%; height: 104px; }
#image-nav-panel .image-side-nav-left-mask { position: absolute; overflow:hidden; top: 0; left: 0; width: 0px; height: 104px; cursor: pointer;}
#image-nav-panel .image-side-nav-right-mask { position: absolute; overflow:hidden; top: 0; right: 0; width: 0px; height: 104px; cursor: pointer;}
#image-nav-panel .image-side-nav-left { position: absolute; overflow:hidden; top: 0; right: 0; width: 168px; height: 104px;}
#image-nav-panel .image-side-nav-right { position: absolute; overflow:hidden; top: 0; left: 0; width: 168px; height: 104px;}
#image-nav-panel .image-side-nav-left .bg { position: absolute; background-image: url('../images/arrow_left.png'); width: 168px; height: 104px;}
#image-nav-panel .image-side-nav-right .bg { position: absolute; background-image: url('../images/arrow_right.png'); width: 168px; height: 104px;}
#image-nav-panel .image-side-nav-left .caption-text { color: #fff; font-family: $headerFontFamily; @include font-size(11); position: absolute; top: 68px; right: 12px; width: 50px; height: 40px; text-align: right;}
#image-nav-panel .image-side-nav-right .caption-text { color: #fff; font-family: $headerFontFamily; @include font-size(11); position: absolute; top: 68px; left: 12px; width: 50px; height: 40px; text-align: left;}
#image-nav-panel .image-side-nav-left .thumb { position: absolute; top: 11px; right: 76px; width: 140px; height: 78px; background-color:#000; border:1px solid #000; overflow:hidden;}
#image-nav-panel .image-side-nav-right .thumb { position: absolute; top: 11px; left: 76px; width: 140px; height: 78px; background-color:#000; border:1px solid #000; overflow:hidden;}

#image-nav-panel .thumb img { width: 140px; height: 78px; }

#image-nav-panel .people-container { position: absolute; top: 0px; left: 0px; width: 2000px;}

#image-nav-panel .person1, 
#image-nav-panel .person2, 
#image-nav-panel .person3 { float: left; }



// video page
#image-nav-panel .thumb img { width: 140px; height: 78px; }







// overriding default values
// main button
.button {background-color:#246; border:0;}
.button:hover {background-color:#71F;}

// responsive grid width
.row { width: 100%; max-width: 980px; min-width: 980px; margin: 0 auto; }




/* videojs */

// overriding videojs player button
.vjs-big-play-button {
	width: 90px !important;
	margin-left: -45px !important;
	height: 90px !important;
	margin-top: -45px !important;

	border: solid 1px #222 !important;

	background-color: #2c2c2c !important; 
	background-image: none !important;
	background-image: none !important;
	background-image: none !important;
	background-image: none !important;
	background-image: none !important;
	background-image: none !important;
	background-repeat: no-repeat !important;
	filter: none !important;

	-moz-box-shadow: 0px 0px 0px #888 !important;
	-webkit-box-shadow: 0px 0px 0px #888 !important;
	box-shadow: 0px 0px 0px #888 !important;

	-webkit-border-radius: 4px 4px !important;
	-moz-border-radius: 4px / 4px !important;
	-khtml-border-radius: 4px / 4px !important;
	border-radius: 4px / 4px !important;
}
.vjs-big-play-button:hover {
	background-color: #111 !important;
}






/**
 * prism.js default theme for JavaScript, CSS and HTML
 * Based on dabblet (http://dabblet.com)
 * @author Lea Verou
 */

code[class*="language-"],
pre[class*="language-"] {color: black;
	text-shadow: 0 1px white;	font-family: Consolas, Monaco, 'Andale Mono', monospace;	direction: ltr;	text-align: left;	white-space: pre;	word-spacing: normal;		-moz-tab-size: 4;	-o-tab-size: 4;	tab-size: 4;	-webkit-hyphens: none;	-moz-hyphens: none;	-ms-hyphens: none;	hyphens: none; background-color: transparent;}

@media print {	code[class*="language-"],	pre[class*="language-"] {		text-shadow: none;	}}

/* Code blocks */
pre[class*="language-"] {	padding: 1em;	margin: .5em 0;	overflow: auto;	}

:not(pre) > code[class*="language-"],
pre[class*="language-"] {	background: #f5f2f0;}

/* Inline code */
:not(pre) > code[class*="language-"] {	padding: .1em;	border-radius: .3em;}

.token.comment,
.token.prolog,
.token.doctype,
.token.cdata {	color: slategray;}

.token.punctuation {	color: #999;}

.namespace {	opacity: .7;}

.token.property,
.token.tag,
.token.boolean,
.token.number {	color: #905;}

.token.selector,
.token.attr-name,
.token.string {	color: #690;}

.token.operator,
.token.entity,
.token.url,
.language-css .token.string,
.style .token.string {	color: #a67f59;	background: hsla(0,0%,100%,.5);}

.token.atrule,
.token.attr-value,
.token.keyword {	color: #07a;}


.token.regex,
.token.important {	color: #e90;}

.token.important {	font-weight: bold;}

.token.entity {	cursor: help;}










